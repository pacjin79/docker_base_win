# guest os
guest_os: trusty

# docker variables
apt_key_sig: 58118E89F3A912897C070ADBF76221572C52609D
apt_key_url: hkp://p80.pool.sks-keyservers.net:80

# Name of the apt repository for docker
apt_repository: deb https://apt.dockerproject.org/repo ubuntu-{{ guest_os }} main

# docker.list file location
docker_list_path: /etc/apt/sources.list.d/docker.list

# guest ansible vars